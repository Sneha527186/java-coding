import java.util.*;
class Cal{
public static void main(String[] args){
Scanner sc=new Scanner(System.in);
int a=10,b=20;
String op;
System.out.println("enter operation");
op=sc.nextLine();
switch(op){
case "add":
System.out.println(a+b);
break;
case "mul":
System.out.println(a*b);
break;
case "sub":
System.out.println(a-b);
break;
case "div":
System.out.println(a/b);
break;
default:
System.out.println("invalid");
}
}
}
