package array;
import java.util.*;

public class Arr {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.print("enter the number:");
		int n=sc.nextInt();
		int[] arr1=new int[n];
		for(int i=0;i<n;i++) {
			
			arr1[i]=sc.nextInt();
			
		}
		for(int i=0;i<arr1.length;i++) {
			if(arr1[i]%2==0) {
				System.out.print(arr1[i]);
			}
			
		}
	}

}
